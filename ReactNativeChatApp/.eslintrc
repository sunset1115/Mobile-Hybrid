{
    "env": {
        "es6": true
    },
    "plugins": [
        "react"
    ],
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
    },
    "extends": "airbnb",
    "rules": {
        "indent": [2, 4],
        "comma-dangle": 0,
        "new-cap": 0,
        "id-length": 0,
        "react/sort-comp": 0,
        "react/no-multi-comp": 0,
        "react/jsx-boolean-value": 0
    }
}
